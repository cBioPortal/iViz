    "grunt-contrib-htmlmin": "^0.4.0",
    "grunt-contrib-imagemin": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-eslint": "^17.3.2",
    "grunt-filerev": "^2.2.0",
    "grunt-json-to-sass": "0.0.3",
    "grunt-mocha": "^0.4.12",
    "grunt-modernizr": "^0.6.0",
